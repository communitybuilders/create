{
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "exports": {
    "./<%= tagName %>.js": "./dist/src/<%= tagName %>.js"
  },
  "scripts": {
    "dev": "<%= scriptRunCommand %> build:style && concurrently -k -r \"<%= scriptRunCommand %> watch:style\" \"vite --open\"",
    "build": "rm -rf dist && <%= scriptRunCommand %> build:style && tsc && <%= scriptRunCommand %> analyze -- --exclude dist",
    "preview": "vite preview",
    "build:style": "node ../../scripts/sass-to-lit-css/index.js *.scss",
    "prepublish": "<%= scriptRunCommand %> build",
    "watch:style": "chokidar --initial \"*.scss\" \"themes/**/*.scss\" -c \"<%= scriptRunCommand %> build:style\""
  },
  "dependencies": {
    "@cbar/cbar-themeable-mixin": "^1.0.2",
    "lit": "^2.8.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2"
  }
}
